{
  "name": "guten-start",
  "author": "elf02 <chris@elf02.de> (https://elf02.de/)",
  "version": "1.0.0",
  "description": "A starter block theme. Have a <guten> start!",
  "license": "GPL-3.0",
  "scripts": {
    "start": "wp-scripts start --webpack-src-dir=resources --output-path=public",
    "build": "wp-scripts build --webpack-src-dir=resources --output-path=public",
    "create-block": "cd ./resources/blocks && npx @wordpress/create-block --namespace=elf02-gust --no-plugin",
	"create-block:dynamic": "cd ./resources/blocks && npx @wordpress/create-block --namespace=elf02-gust --no-plugin --variant=dynamic"
  },
  "devDependencies": {
    "@wordpress/scripts": "^28.5.0",
    "copy-webpack-plugin": "^12.0.2",
    "glob": "^11.0.0",
    "path": "^0.12.7",
    "webpack-remove-empty-scripts": "^1.0.4"
  }
}
